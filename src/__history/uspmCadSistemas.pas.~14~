unit uspmCadSistemas;

interface

uses
  System.SysUtils, System.Classes, Vcl.Controls, Vcl.Forms, uspmFormCadPadrao,
  Vcl.StdCtrls, Vcl.DBCtrls, JvDBCheckBox, JvGroupBox, JvExStdCtrls, Vcl.Mask,
  Data.DB, System.Actions, Vcl.ActnList, Vcl.ImgList, JvLabel, JvDBControls, Vcl.Grids,
  Vcl.DBGrids, JvExDBGrids, JvDBGrid, Vcl.ComCtrls, JvExControls, JvButton,
  JvTransparentButton, Vcl.ExtCtrls;

type
  TspmCadSistemas = class(TspmFormCadPadrao)
    Label1: TLabel;
    edDescricao: TDBEdit;
    JvGroupBox1: TJvGroupBox;
    JvDBCheckBox1: TJvDBCheckBox;
    Label2: TLabel;
    JvDBCheckBox2: TJvDBCheckBox;
    JvDBCheckBox3: TJvDBCheckBox;
    JvDBCheckBox4: TJvDBCheckBox;
    Label3: TLabel;
    DBEdit1: TDBEdit;
    procedure FormCreate(Sender: TObject);
    procedure FormDestroy(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  spmCadSistemas: TspmCadSistemas;

implementation

{$R *.dfm}

procedure TspmCadSistemas.FormCreate(Sender: TObject);
begin
  try
    try
      inherited;

      Self.FoFormulario := Self;
    except
      on E: Exception do
      begin
        raise Exception.Create(E.Message);
      end;
    end;
  finally

  end;
end;

procedure TspmCadSistemas.FormDestroy(Sender: TObject);
begin
  try
    try
      inherited;

      spmCadSistemas := nil;
    except
      on E: Exception do
      begin
        raise Exception.Create(E.Message);
      end;
    end;
  finally

  end;
end;

end.

